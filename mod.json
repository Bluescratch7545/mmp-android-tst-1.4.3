{
	"geode": "4.10.0",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "bluescratch.main_menu_plus",
	"name": "Main Menu Plus",
	"version": "v1.4.3",
	"developer": "Bluescratch",
	"description": "A Mod Which Adds Animations To The GD Menu!",
	"dependencies": {
		"geode.node-ids": {
			"version": ">=1.20.0",
			"importance": "required"
		},
		"cvolton.misc_bugfixes": {
			"importance": "suggested",
			"version": ">=1.8.0"
		},
		"geode.custom-keybinds": {
			"version": ">=v1.0.0",
			"importance": "required",
			"platforms": [
				"android",
				"mac",
				"win"
			]
		}
	},

	"tags": [
		"interface", "customization", "utility"
	],
	"settings": {
		"mod": {
			"type": "title",
			"name": "Other Mod Settings"
		},
		"enable-mod": {
			"default": true,
			"name": "Enable Mod",
			"description": "Enables/Disables the Mod without restarting",
			"type": "bool"
		},
		"main-menu-plus-settings": {
			"type": "title",
			"name": "Settings"
		},
		"toggle-moreGames": {
			"type":"bool",
			"name": "Toggle More Games",
			"description": "Toggles The More Games Button",
			"default": false
		},
		"toggle-links":{
			"name": "Toggle Social Media Menu",
			"description": "toggles the social media menu",
			"default": false,
			"type": "bool"
		},
		"btn-repos":{
			"default": false,
			"name": "Info & Settings Btn Reposition",
			"description": "(Temporary) Repositions the mod info and mod settings button.",
			"type": "bool"
		},
		"4x3Fix": {
			"type": "bool",
			"default": false,
			"name": "4:3 Fix",
			"description": "BugFix For The 4:3 screen ratio bug",
			"platforms": [
				"mobile"
			]
		},
		"disable-info": {
			"type": "bool",
			"default": false,
			"description": "If On, Disables The Extra Mod Info Btn",
			"name": "Disable Info Btn",
			"enable-if": "enable-mod",
			"enable-if-description": "You Cannot Change This While The Mod Is Off!"
		},
		"slide-value-settings": {
			"type": "title",
			"name": "Slide Settings"
		},
		"slide-speed": {
			"type": "int",
			"default": 2,
			"name": "Anim Speed",
			"description": "Sets The Speed Of The Animations",
			"min": 1,
			"max": 6,
			"control": {
				"arrow-step": 1,
				"arrows": true,
				"slider": true,
				"slider-step": 1,
				"input": true
			},
			"enable-if": "enable-change-slide"
		},

		"enable-change-slide": {
			"default": false,
			"name": "Enable Slide Value Changing",
			"description": "Enables The Changing Of The Slide Speed",
			"type": "bool"
		},
		"slide-type": {
			"type": "string",
			"one-of": ["Bounce Out", "Exponential Out", "Back Out"],
			"default": "Back Out",
			"name": "Slide Type",
			"description": "What The Slide Type Should Be"
		},
		"support-options": {
			"type": "title",
			"name": "Support Options"
		},
		"redash-support": {
			"type": "bool",
			"default": false,
			"name": "OMM Support",
			"description": "Adds Support For Overcharged Main Menu <cr> WARNING: STILL IN BETA"
		}
	},
	"links": {
		"source": "https://github.com/Bluescratch7545/Main-Menu-Plus"
	}
}